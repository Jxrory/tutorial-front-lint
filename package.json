		"@typescript-eslint/eslint-plugin": "^5.57.0",
		"@typescript-eslint/parser": "^5.57.0",
		"@vitejs/plugin-vue": "^4.0.0",
		"@vitejs/plugin-vue-jsx": "^3.0.0",
		"@vue/tsconfig": "^0.1.3",
		"cz-git": "^1.6.1",
		"eslint": "^8.37.0",
